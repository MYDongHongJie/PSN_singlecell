proc ModulesHelp { } {
        global dotversion

        puts stderr "\tAdds '/public/dev_scRNA/software/conda_envs/STRS_envs/bin' to your PATH environment variable"
        puts stderr "\tto your PATH environment variable.  This allows you to"
        puts stderr "\trun executables from the /public/dev_scRNA/software/conda_envs/STRS_envs/bin."
}

module-whatis   "adds '/public/dev_scRNA/software/conda_envs/STRS_envs/bin' to your PATH environment variable"
prepend-path    PATH    /public/dev_scRNA/software/conda_envs/STRS_envs/bin
prepend-path    PATH    /public/dev_scRNA/software/conda_envs/STRS_envs/lib/R/library/OESingleCell/exec
#prepend-path    PATH /public/dev_scRNA/xfyang/devlopment_project/oesinglecell3/exec
prepend-path -d " " CFLAGS "-I/public/dev_scRNA/software/conda_envs/STRS_envs/include"
prepend-path -d " " CPPFLAGS "-I/public/dev_scRNA/software/conda_envs/STRS_envs/include"
setenv   R_LIBS_USER  "/public/dev_scRNA/software/conda_envs/STRS_envs/lib/R/library"
prepend-path -d " " LDFLAGS "-L/public/dev_scRNA/software/conda_envs/STRS_envs/lib"
prepend-path    LD_RUN_PATH /public/dev_scRNA/software/conda_envs/STRS_envs/lib
prepend-path   CONDA_ENV  "/public/dev_scRNA/software/conda_envs/STRS_envs"
append-path    LD_LIBRARY_PATH /public/dev_scRNA/software/conda_envs/STRS_envs/lib
#prepend-path    ST_scripdir "/home/xfyang/pipeline/Visium_develop/Visium"
prepend-path    PYTHONPATH /public/dev_scRNA/software/conda_envs/STRS_envs/lib/python3.11/site-packages
prepend-path    LD_LIBRARY_PATH /public/dev_scRNA/software/conda_envs/STRS_envs/jre/lib/amd64/server
append-path     PATH /public/dev_scRNA/software/conda_envs/STRS_envs/lib/python3.11/site-packages
module load pigz 
prereq  pigz 
