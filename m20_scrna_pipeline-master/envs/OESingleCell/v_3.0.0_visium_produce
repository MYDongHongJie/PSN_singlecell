proc ModulesHelp { } {
        global dotversion

        puts stderr "\tAdds '/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/bin' to your PATH environment variable"
        puts stderr "\tto your PATH environment variable.  This allows you to"
        puts stderr "\trun executables from the /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/bin."
}

module-whatis   "adds '/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/bin' to your PATH environment variable"
prepend-path    PATH    /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/bin
prepend-path    PATH    /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib/R/library/OESingleCell/exec
#prepend-path    PATH /public/dev_scRNA/xfyang/devlopment_project/oesinglecell3/exec
prepend-path -d " " CFLAGS "-I/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/include"
prepend-path -d " " CPPFLAGS "-I/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/include"
setenv   R_LIBS_USER  "/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib/R/library"
prepend-path -d " " LDFLAGS "-L/public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib"
prepend-path    LD_RUN_PATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib
prepend-path    LD_LIBRARY_PATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib
prepend-path    LD_LIBRARY_PATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce_advance/lib
#prepend-path    ST_scripdir "/home/xfyang/pipeline/Visium_develop/Visium"
prepend-path    PYTHONPATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib/python3.11/site-packages
prepend-path    LD_LIBRARY_PATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/jre/lib/amd64/server
append-path     PATH /public/dev_scRNA/software/miniconda3/envs/OESingleCell_3.00_visium_produce/lib/python3.11/site-packages
module load pigz
prereq  pigz
