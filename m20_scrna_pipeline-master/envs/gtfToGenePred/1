proc ModulesHelp { } {
        global dotversion

        puts stderr "\tAdds `/public/dev_scRNA/xfyang/M20-RNAseq/demo_1/genome/' to your PATH environment variable"
        puts stderr "\tto your PATH environment variable.  This allows you to"
        puts stderr "\trun executables from the /public/dev_scRNA/xfyang/M20-RNAseq/demo_1/genome/"
}

module-whatis   "adds `/public/dev_scRNA/xfyang/M20-RNAseq/demo_1/genome/' to your PATH environment variable"

prepend-path    PATH    /public/dev_scRNA/xfyang/M20-RNAseq/demo_1/genome/
